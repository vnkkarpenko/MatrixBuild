<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MatrixBuild</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/reactjs")
    @*"~/bundles/modernizr","~/bundles/jquery", "~/bundles/bootstrap",*@
</head>
<body>
    <div class="container">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - ASP.NET + React JS Application</p>
        </footer>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
